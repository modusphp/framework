{
    "name": "modus/framework",
    "description": "The Modus framework, providing basic support for many components of Aura and other libraries.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.0",
        "aura/session": "~1.0.1",
        "aura/di": "~2.1",
        "aura/sql" : "~2.0",
        "aura/sqlquery": "~2.0",
        "aura/router": "~2.0",
        "aura/view": "~2.0.0",
        "aura/web": "~2.0",
        "monolog/monolog": "~1.5",
        "pagerfanta/pagerfanta": "v1.0.1",
        "modus/auth": "~1.0",
        "aura/html": "~2.1.0",
        "league/booboo": "~1.0",
        "aura/accept": "~2.0",
        "modus/config": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.0"
    },
    "autoload": {
        "psr-4": {
            "Modus\\": "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/modusphp/auth"
        }
    ]
}
