{
    "name": "tailwindsllc/modus",
    "description": "The Modus framework, providing basic support for many components of Aura and other libraries.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "beta",
    "require": {
        "php": ">=5.4.0",
        "aura/session": "~1.0.1",
        "aura/di": "~2.1",
        "aura/sql" : "~2.0",
        "aura/sqlquery": "~2.0",
        "aura/router": "~2.0",
        "aura/view": "~2.0.0",
        "aura/http": "~1.0.1",
        "aura/web": "~2.0",
        "aura/cli": "~2.0",
        "monolog/monolog": "~1.5",
        "zendframework/zend-escaper" : "~2.2",
        "pagerfanta/pagerfanta": "v1.0.1",
        "league/flysystem": "~0.5",
        "robmorgan/phinx": "v0.3.2",
        "aura/auth": "2.0.0-beta2",
        "aura/html": "~2.1.0",
        "brandonsavage/booboo": "~0.2",
        "ircmaxell/password-compat": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.0"
    },
    "autoload": {
        "psr-4": {
            "Modus\\": "src"
        },
        "psr-0": {
            "Application": "local-src/"
        }
    },
    "bin": [
        "bin/modus"
    ]
}